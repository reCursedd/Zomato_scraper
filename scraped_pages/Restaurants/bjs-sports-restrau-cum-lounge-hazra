    <div class="  ui segments res-review-body res-review clearfix js-activity-root   item-to-hide-parent stupendousact" data-review_id="28156224" data-snippet="restaurant-review" data-res_id="18265479">
        <div class="ui segment clearfix  brtop ">
            
            <div class="ui warning message error-message-highlight review-message hidden mbot"></div>

            <div class="ui item clearfix ">
                                <div class="item">
                    <div class="clearfix  mb10 ">
                        <div class="res-large-snippet ui horizontal list col-s-16  ">

  <div class="item col-s-16">
    <a href="https://www.zomato.com/ShreyaBhowmick">
            <img class="ui avatar image left tiny" alt="Shreya Bhowmick" src="https://b.zmtcdn.com/images/placeholder_200.png" data-original="https://b.zmtcdn.com/data/user_profile_pictures/48e/f9cc31c40aa3486a9af1a95acd65f48e.jpg?fit=around%7C100%3A100&amp;crop=100%3A100%3B%2A%2C%2A" />
          </a>


<!--      -->

    <div class="content col-l-11 large-snippet ml5 mt5">

      <div class="header nowrap ui left">
                  <a href="https://www.zomato.com/ShreyaBhowmick" data-entity_id="2526344">
            Shreya Bhowmick
          </a>
                
      </div>
                <div class="clear"></div>

      <span class="grey-text fontsize5 nowrap">

                        
                                            246 Reviews
                                ,
                          819 Followers
                                          
              </span>
        <div class="clear"></div>

                              <div class="zs-follow-btn-container usr-page-follow-btn clear mt2" data-entity-type="USER" data-user-id="2526344" data-is-followed="">
                <a role="button" aria-label="Follow" class="social-button follow-social cgrey ui basic large label ui-label-bb zs-follow-user-button">Follow</a>
