    <div class="  ui segments res-review-body res-review clearfix js-activity-root   item-to-hide-parent stupendousact" data-review_id="25357875" data-snippet="restaurant-review" data-res_id="18016829">
        <div class="ui segment clearfix  brtop ">
            
            <div class="ui warning message error-message-highlight review-message hidden mbot"></div>

            <div class="ui item clearfix ">
                                <div class="item">
                    <div class="clearfix  mb10 ">
                        <div class="res-large-snippet ui horizontal list col-s-16  ">

  <div class="item col-s-16">
    <a href="https://www.zomato.com/joyjeetgupta">
            <img class="ui avatar image left tiny" alt="Joyjeet Gupta" src="https://b.zmtcdn.com/images/placeholder_200.png" data-original="https://b.zmtcdn.com/data/user_profile_pictures/75f/66626f23c47a6a094be5b365f37e375f.jpg?fit=around%7C100%3A100&amp;crop=100%3A100%3B%2A%2C%2A" />
          </a>


<!--      -->

    <div class="content col-l-11 large-snippet ml5 mt5">

      <div class="header nowrap ui left">
                  <a href="https://www.zomato.com/joyjeetgupta" data-entity_id="557729">
            Joyjeet Gupta
          </a>
                
      </div>
                <div class="clear"></div>

      <span class="grey-text fontsize5 nowrap">

                        
                                            98 Reviews
                                ,
                          840 Followers
                                          
              </span>
        <div class="clear"></div>

                              <div class="zs-follow-btn-container usr-page-follow-btn clear mt2" data-entity-type="USER" data-user-id="557729" data-is-followed="">
                <a role="button" aria-label="Follow" class="social-button follow-social cgrey ui basic large label ui-label-bb zs-follow-user-button">Follow</a>
