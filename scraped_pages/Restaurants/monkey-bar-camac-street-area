<!--      -->

    <div class="content col-l-11 large-snippet ml5 mt5">

      <div class="header nowrap ui left">
                  <a href="https://www.zomato.com/mohamushkil" data-entity_id="542545">
            Indrajit Lahiri
          </a>
                
      </div>
                <div class="clear"></div>

      <span class="grey-text fontsize5 nowrap">

                        
                                            54 Reviews
                                ,
                          435 Followers
                                          
              </span>
        <div class="clear"></div>

                              <div class="zs-follow-btn-container usr-page-follow-btn clear mt2" data-entity-type="USER" data-user-id="542545" data-is-followed="">
                <a role="button" aria-label="Follow" class="social-button follow-social cgrey ui basic large label ui-label-bb zs-follow-user-button">Follow</a>
