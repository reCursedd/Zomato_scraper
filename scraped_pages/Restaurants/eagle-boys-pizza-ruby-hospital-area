                    if(address.status) { 
                        if (!isMobile) {
                            address.alias = RemoveHTMLTagsImproved(address.alias);
                        }
                        %&gt;
                    &lt;li class="save-address" data-address_id="&lt;%= address.id %&gt;"&gt;
                        &lt;div class="ui radio checkbox block-label"&gt;
