    <div class="content col-l-11 large-snippet ml5 mt5">

      <div class="header nowrap ui left">
                  <a href="https://www.zomato.com/mukulita" data-entity_id="207896">
            Mukulita Ganguly
          </a>
                
      </div>
                <div class="clear"></div>

      <span class="grey-text fontsize5 nowrap">

                        
                                            144 Reviews
                                ,
                          1127 Followers
                                          
              </span>
        <div class="clear"></div>

                              <div class="zs-follow-btn-container usr-page-follow-btn clear mt2" data-entity-type="USER" data-user-id="207896" data-is-followed="">
                <a role="button" aria-label="Follow" class="social-button follow-social cgrey ui basic large label ui-label-bb zs-follow-user-button">Follow</a>
